<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true" >
        <shape>
            <solid
                android:color="#605d5e" />
            <stroke
                android:width="1dp"
                android:color="#000000" />
            <corners
                android:radius="3dp" />
            <padding
                android:left="10dp"
                android:top="10dp"
                android:right="10dp"
                android:bottom="10dp" />
        </shape>
    </item>
    <item>
        <shape>


            <stroke
                android:width="1dp"
                android:color="#000000">
            </stroke>
            <corners
                android:radius="3dp"
                >

            </corners>
        </shape>
    </item>
</selector>